record(ao, "$(P)CON1")
{
   field(DTYP, "asynFloat64")
   field(OUT,  "@asyn(ex1,0,0)cont1")
   field(PREC, "3")
}

record(ai, "$(P)IND1")
{
   field(DTYP, "asynInt32")
   field(INP,  "@asyn(ex1,0,0)ind1")
   field(PREC, "3")
   field(SCAN, ".1 second")
}

record(waveform, "$(P)FLTARRAY")
{
   field(DTYP, "asynFloat64ArrayIn")
   field(INP,  "@asyn(ex1,0,0)arrayind1")
   field(PREC, "3")
   field(SCAN, ".1 second")
   field(NELM, "10")
   field(FTVL, "DOUBLE")
}

record(stringin, "$(P)STR")
{
   field(DTYP, "asynOctetRead")
   field(INP,  "@asyn(ex1,0,0)strcont1")
   field(SCAN, ".1 second")
}

record(stringout, "$(P)STRW")
{
   field(DTYP, "asynOctetWrite")
   field(OUT,  "@asyn(ex1,0,0)strcont1")
}

