<?xml version="1.0" encoding="UTF-8"?>
<lvinput>
   <extint path="c:/LabVIEW Modules old/Common/External Interface/External Interface.llb/External Interface - Set Value.vi" />
   <port name="beamlogger"> <!-- name will match a "port" of EPICS asyn driver -->
    <vi path="c:/development/instrumentcontrol/icp/labview modules/beam logger/beam logger.vi">
      <param name="TS1Beam" type="float64"> <!-- name will match a parameter of EPICS asyn driver -->
        <read method="GCV" target="TS1 Beam Current" />   <!-- target is the name of a control or indicator on the labview Vi -->
        <set method="SCV" extint="true" target="TS1 Beam Current" /> <!-- target is the name of a control on the labview Vi -->
      </param>
      <param name="TS2Beam" type="float64"> <!-- name will match a parameter of EPICS asyn driver -->
        <read method="GCV" target="TS2 Beam Current" />
        <set method="SCV" extint="false" target="TS2 Beam Current" />
      </param>
      <param name="array" type="float64array"> <!-- name will match a parameter of EPICS asyn driver -->
        <read method="GCV" target="testarray" />
        <set method="SCV" extint="false" target="testarray" />
      </param>
      <param name="Time" type="string"> <!-- name will match a parameter of EPICS asyn driver -->
        <read method="GCV" target="Last Update Time" />
        <set method="SCV" extint="false" target="Last Update Time" />
      </param>
    </vi>
  </port>
</lvinput>
